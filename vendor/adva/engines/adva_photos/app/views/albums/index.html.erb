<% if collection_title(@set, @tags) %>
  <div class="pageHeader"><%= collection_title(@set, @tags) %></div>
<% end %>
<div id="setList">
<ul>
<% @section.sets.each do |set| %>
          <li><%= link_to_set @section, set %></li>
<% end %>
</ul>
</div>
<br>
<div id="photoContainer">
<%= render :partial => 'photo', :collection => @photos, :locals => {:mode => :many} %>
</div>
<%= render :partial => 'footer' %>